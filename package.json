  "scripts": {
    "ganache": "npx ganache-cli",
    "start": "env-cmd -f .env.local react-scripts start",
    "start:testnet": "env-cmd -f .env.testnet react-scripts start",
    "start:mainnet": "env-cmd -f .env.production react-scripts start",
    "build:testnet": "env-cmd -f .env.testnet react-scripts --max_old_space_size=8192 build",
    "build:mainnet": "env-cmd -f .env.production react-scripts --max_old_space_size=8192 build",
    "test": "node ./tests/setupRnsSuite.js && jest --maxWorkers=4 && eslint src",
    "test:watch": "node ./tests/setupRnsSuite.js && jest --watch",
    "cypress": "cypress open",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "cypress": "^6.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "7.14.3",
    "ganache-cli": "^6.12.2",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.2",
    "redux-mock-store": "^1.5.4"
  },
  "resolutions": {
    "lodash": "^4.17.14",
    "lodash.template": "^4.5.0",
    "eslint-utils": "^1.4.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./tests/setupTests.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testEnvironment": "enzyme",
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(gif|png|eot|svg)$": "<rootDir>/tests/config/mockImage.js"
    },
    "verbose": false,
    "setupFiles": [
      "<rootDir>/tests/setEnvVars.js"
    ]
  }
}
